<%= form_for(@album, :html => { :multipart => true }) do |f| %>
   <div class="field">
     <%= f.label :titre %><br>
     <%= f.text_field :title %>
   </div>

   <%= f.fields_for :album_attachments do |p| %>
     <div class="field">
       <%= p.label :images %><br>
       <%= p.file_field :picture, :multiple => true, name: "album_attachments[picture][]" %>
     </div>
   <% end %>
   <br/>
   <div class="actions">
     <%= f.submit "CrÃ©er album", class:"waves-effect waves-light btn" %>
   </div>
<% end %>
